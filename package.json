{
  "author": "CRF Health",
  "name": "webstudio-docs",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "engines": {
    "node": ">=5.0 <7",
    "npm": ">=3.3"
  },
  "scripts": {
    "build": "webpack --color -p --display-error-details --config internals/webpack/prod.config.js",
    "dev": "concurrently --kill-others \"npm run watch-client\" \"npm run start-dev\"",
    "watch-client": "webpack-dev-server --verbose --colors --display-error-details --config internals/webpack/dev.config.js",
    "prestart": "babel ./server --out-dir build/",
    "prod": "npm start",
    "start": "cross-env NODE_ENV=production node ./build/bin/www.js --port 7050",
    "start-dev": "cross-env NODE_ENV=development nodemon --exec babel-node ./server/bin/www.js --port 7000",
    "lint": "eslint ./client/src ./client/tests --ext .js --ext .jsx --ignore-pattern dist --cache",
    "test-client": "mocha --compilers js:babel-core/register --require ignore-styles --require ./client/test/test_setup.js \"./client/**/*@(test.js|test.jsx)\"",
    "test-client-watch": "npm run test-client -- --watch --watch-extensions jsx"
  },
  "dependencies": {
    "bluebird": "3.4.6",
    "body-parser": "^1.15.2",
    "chalk": "^1.1.3",
    "compression": "^1.6.2",
    "cross-env": "^3.0.0",
    "ejs": "^2.5.1",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "helmet": "2.3.0",
    "history": "^3.0.0",
    "html-webpack-plugin": "^2.22.0",
    "immutable": "^3.8.1",
    "ip": "^1.1.3",
    "json-loader": "^0.5.4",
    "minimist": "^1.2.0",
    "morgan": "^1.7.0",
    "pg-promise": "5.3.4",
    "react": "^15.3.0",
    "react-addons-pure-render-mixin": "^15.3.0",
    "react-bootstrap": "^0.30.3",
    "react-css-modules": "^3.7.10",
    "react-dom": "^15.3.0",
    "react-redux": "^4.4.5",
    "react-router": "^2.6.1",
    "react-router-bootstrap": "^0.23.1",
    "redux": "^3.6.0",
    "reselect": "^2.5.3",
    "serve-favicon": "2.3.0",
    "winston": "^2.2.0"
